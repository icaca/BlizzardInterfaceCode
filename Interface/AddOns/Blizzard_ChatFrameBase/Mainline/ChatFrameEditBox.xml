	<EditBox name="ChatFrameEditBoxTemplate" mixin="ChatFrameEditBoxMixin" inherits="AutoCompleteEditBoxTemplate" autoFocus="false" ignoreArrows="true" frameStrata="DIALOG" toplevel="true" historyLines="32" bytes="1280" visibleBytes="255" invisibleBytes="1023" hidden="true" virtual="true">
