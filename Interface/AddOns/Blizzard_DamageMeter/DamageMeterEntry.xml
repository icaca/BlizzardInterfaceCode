<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\Blizzard_SharedXML\UI.xsd">
	<Button name="DamageMeterEntryTemplate" mixin="DamageMeterEntryMixin" virtual="true" clipChildren="true">
		<Size y ="24"/>
		<Frames>
			<Frame parentKey="Icon">
				<Size x="24" y="24"/>
				<Anchors>
					<Anchor point="LEFT"/>
				</Anchors>
				<Layers>
					<Layer level="ARTWORK">
						<Texture parentKey="Icon" setAllPoints="true">
							<TexCoords top="0.0626" left="0.0625" bottom="0.9" right="0.9"/>
						</Texture>
					</Layer>
				</Layers>
			</Frame>
			<StatusBar parentKey="StatusBar">
				<!-- Anchoring dynamically managed in Lua. -->
				<BarTexture atlas="UI-HUD-CoolDownManager-Bar" setAllPoints="true"/>
				<Layers>
					<Layer level="BACKGROUND">
						<Texture parentKey="Background" parentArray="BackgroundRegions" atlas="ui-damagemeters-bar-shadowbg">
							<!-- Anchoring dynamically managed in Lua. -->
						</Texture>
					</Layer>
					<Layer level="OVERLAY">
						<Texture parentKey="BackgroundEdge" parentArray="BackgroundRegions" atlas="ui-damagemeters-bar-shadowedge">
							<Anchors>
								<Anchor point="TOPLEFT" x="-2" y="2"/>
								<Anchor point="BOTTOMRIGHT" x="2" y="-2"/>
							</Anchors>
						</Texture>
					</Layer>
					<Layer level="OVERLAY">
						<FontString parentKey="Value" inherits="NumberFontNormal" justifyH="LEFT">
							<!-- Anchoring dynamically managed in Lua. -->
						</FontString>
						<FontString parentKey="Name" inherits="NumberFontNormal" justifyH="LEFT" wordwrap="false">
							<!-- Anchoring dynamically managed in Lua. -->
						</FontString>
					</Layer>
				</Layers>
			</StatusBar>
		</Frames>
	</Button>

	<Button name="DamageMeterSourceEntryTemplate" inherits="DamageMeterEntryTemplate" mixin="DamageMeterSourceEntryMixin" virtual="true"/>
	<Button name="DamageMeterSpellEntryTemplate" inherits="DamageMeterEntryTemplate" mixin="DamageMeterSpellEntryMixin" virtual="true"/>
</Ui>
