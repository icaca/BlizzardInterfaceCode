							<Color color="ACCOUNT_WIDE_FONT_COLOR"/>
							<Anchors>
								<Anchor point="TOP" relativeKey="$parent.JourneyName" relativePoint="BOTTOM" y="-5"/>
								<Anchor point="CENTER"/>
							</Anchors>
						</FontString>
						<FontString parentKey="JourneyDescription" justifyV="TOP" inherits="GameFontNormalMed2">
							<Size x="620" y="70"/>
							<Anchors>
								<Anchor point="CENTER"/>
							</Anchors>
						</FontString>
						<Texture parentKey="DividerTexture" atlas="ui-journeys-renown-divider" useAtlasSize="true">
							<Anchors>
								<Anchor point="CENTER"/>
								<Anchor point="TOP" relativeKey="$parent.JourneyDescription" relativePoint="BOTTOM"/>
