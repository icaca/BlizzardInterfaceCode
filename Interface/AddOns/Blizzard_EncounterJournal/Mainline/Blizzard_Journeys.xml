									<Anchor point="TOP"/>
									<Anchor point="LEFT"/>
								</Anchors>
								<Scripts>
									<OnShow method="OnShow"/>
									<OnEnter method="OnEnter"/>
									<OnLeave method="OnLeave"/>
									<OnClick method="OnClick"/>
								</Scripts>
								<Layers>
									<Layer level="OVERLAY" textureSubLevel="1">
										<Texture parentKey="Icon">
											<Size x="48" y="48"/>
											<Anchors>
												<Anchor point="LEFT" x="35"/>
											</Anchors>
										</Texture>

										<FontString parentKey="CompanionName" inherits="GameFontHighlightMedium" justifyH="LEFT">
											<Size x="200" y="25"/>
											<Anchors>
												<Anchor point="TOPLEFT" relativeKey="$parent.Icon" relativePoint="TOPRIGHT" x="20" y="5"/>
											</Anchors>
										</FontString>
									</Layer>
									<Layer level="OVERLAY">
										<Texture parentKey="IconBorder" atlas="ui-journeys-delve-companion-ring" useAtlasSize="true">
											<Anchors>
												<Anchor point="CENTER" relativeKey="$parent.Icon"/>
											</Anchors>
										</Texture>
									</Layer>
								</Layers>
							</Button>
						</Frames>
					</Frame>
					<Button parentKey="LevelSkipButton" inherits="RewardTrackSkipLevelUpButtonTemplate" hidden="true" frameLevel="510">
						<Anchors>
							<Anchor point="BOTTOM" x="0" y="8"/>
						</Anchors>
					</Button>
					<ModelScene parentKey="LevelModelScene" inherits="ScriptAnimatedModelSceneTemplate" frameLevel="100">
						<Size x="300" y="300"/>
						<Anchors>
							<Anchor point="CENTER" relativeKey="$parent.RenownTrackFrame"/>
						</Anchors>
					</ModelScene>
				</Frames>
				<Scripts>
					<OnLoad method="OnLoad"/>
					<OnShow method="OnShow"/>
					<OnHide method="OnHide"/>
					<OnMouseWheel method="OnMouseWheel"/>
				</Scripts>
			</Frame>
