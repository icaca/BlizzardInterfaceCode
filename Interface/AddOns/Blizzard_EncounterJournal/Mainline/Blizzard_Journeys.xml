	<!-- Renown Card Button -->
	<Button name="RenownCardButtonTemplate" inherits="AlphaHighlightButtonTemplate" mixin="RenownCardButtonMixin" virtual="true">
		<Size x="374" y="112"/>
		<NormalTexture parentKey="NormalTexture" atlas="ui-journeys-renown-button" useAtlasSize="true"/>
		<PushedTexture parentKey="PushedTexture" atlas="ui-journeys-renown-button-pressed" useAtlasSize="true"/>
		<Frames>
			<Frame parentKey="LockFrame">
				<Size x="40" y="40"/>
				<Anchors>
					<Anchor point="TOPRIGHT" x="-15" y="-10"/>
				</Anchors>
				<Layers>
					<Layer level="OVERLAY">
						<Texture parentKey="LockIcon" atlas="ui-journeys-greatvault-lock" useAtlasSize="true" hidden="true">
							<Anchors>
								<Anchor point="CENTER"/>
							</Anchors>
						</Texture>
					</Layer>
				</Layers>
			</Frame>
			<Frame parentKey="IconFrame">
				<Size x="60" y="60"/>
				<Anchors>
					<Anchor point="LEFT" x="20"/>
				</Anchors>
				<Layers>
					<Layer level="OVERLAY" textureSubLevel="1">
						<Texture parentKey="Border" atlas="ui-journeys-renown-radial-bar" setAllPoints="true" useAtlasSize="false">
							<Size x="60" y="60"/>
							<Anchors>
								<Anchor point="CENTER"/>
							</Anchors>
						</Texture>
					</Layer>
					<Layer level="OVERLAY">
						<Texture parentKey="Icon" useAtlasSize="false">
							<Size x="40" y="40"/>
							<Anchors>
								<Anchor point="CENTER"/>
							</Anchors>
						</Texture>
					</Layer>
				</Layers>
			</Frame>
			<Cooldown parentKey="RenownCardProgressBar" mixin="JourneysProgressBarMixin" reverse="true" hideCountdownNumbers="true" rotation="180" frameLevel="5">
				<Size x="60" y="60"/>
				<Anchors>
					<Anchor point="CENTER" relativeKey="$parent.IconFrame"/>
				</Anchors>
				<SwipeTexture file="Interface\PVPFrame\pvpqueue-sidebar-honorbar-fill"/>
			</Cooldown>

			<Frame parentKey="WatchedFactionToggleFrame" hidden="true">
				<Size x="200" y="20"/>
				<Anchors>
					<Anchor point="RIGHT" x="30" y="-30"/>
				</Anchors>
				<Frames>
					<CheckButton parentKey="WatchFactionCheckbox" inherits="UICheckButtonTemplate" mixin="WatchedFactionToggleFrameMixin">
						<Size x="26" y="26"/>
						<Anchors>
							<Anchor point="LEFT"/>
						</Anchors>
						<HitRectInsets left="0" right="0" top="0" bottom="0"/>
						<Layers>
							<Layer level="ARTWORK">
								<FontString parentKey="Label" inherits="GameFontNormalSmall" text="SHOW_FACTION_ON_MAINSCREEN">
									<Anchors>
										<Anchor point="LEFT" relativePoint="RIGHT" x="-2" y="0"/>
									</Anchors>
								</FontString>
							</Layer>
						</Layers>
						<Scripts>
							<OnShow method="OnShow"/>
							<OnClick method="OnClick"/>
							<OnEnter method="OnEnter"/>
							<OnLeave method="OnLeave"/>
						</Scripts>
					</CheckButton>
				</Frames>
			</Frame>
		</Frames>
		<Layers>
			<Layer level="OVERLAY">
				<FontString parentKey="RenownCardFactionName" inherits="GameFontHighlightMed2" justifyH="LEFT" justifyV="TOP">
					<Size x="225" y="20"/>
					<Anchors>
						<Anchor point="LEFT" relativeKey="$parent.IconFrame" relativePoint="RIGHT" x="5" y="5"/>
					</Anchors>
				</FontString>
				<FontString parentKey="RenownCardFactionLevel" inherits="GameFontNormalMed2" justifyH="LEFT" justifyV="TOP">
					<Size x="220" y="15"/>
					<Anchors>
						<Anchor point="LEFT" relativeKey="$parent.IconFrame" relativePoint="RIGHT" x="5"/>
						<Anchor point="TOP" relativeKey="$parent.RenownCardFactionName" relativePoint="BOTTOM"/>
					</Anchors>
				</FontString>
			</Layer>
		</Layers>
		<Scripts>
			<OnEnter method="OnEnter"/>
			<OnLeave method="OnLeave"/>
			<OnClick method="OnClick"/>
		</Scripts>
	</Button>

