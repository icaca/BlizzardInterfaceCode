				<Layers>
					<Layer level="BACKGROUND">
						<Texture parentKey="IconBorder" atlas="ui-journeys-delve-renown-circle-pit" useAtlasSize="true">
							<Size x="75" y="75"/>
							<Anchors>
								<Anchor point="TOP" y="-20"/>
								<Anchor point="CENTER"/>
							</Anchors>
						</Texture>
						<Texture parentKey="ProgressBorder" atlas="ui-journeys-renown-radial-bar" setAllPoints="true" useAtlasSize="false">
