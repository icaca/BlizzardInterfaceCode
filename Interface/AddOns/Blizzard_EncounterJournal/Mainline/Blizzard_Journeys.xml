							<Anchors>
								<Anchor point="CENTER" relativeKey="$parent.IconBorder"/>
							</Anchors>
						</Texture>
					</Layer>
					<Layer level="ARTWORK">
						<Texture parentKey="JourneyIcon">
							<Size x="50" y="50"/>
							<Anchors>
								<Anchor point="CENTER" relativeKey="$parent.IconBorder"/>
							</Anchors>
						</Texture>
						<FontString parentKey="JourneyName" inherits="GameFontHighlightHuge2">
							<Size x="500" y="25"/>
							<Anchors>
								<Anchor point="TOP" relativeKey="$parent.JourneyIcon" relativePoint="BOTTOM" y="-25"/>
								<Anchor point="CENTER"/>
							</Anchors>
						</FontString>
