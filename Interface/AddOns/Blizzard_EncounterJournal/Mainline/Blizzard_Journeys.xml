	<!-- Journeys Frame -->
	<Frame name="JourneysFrameTemplate" mixin="JourneysFrameMixin" virtual="true">
		<Frames>
			<Frame parentKey="JourneysList" inherits="WowScrollBoxList" setAllPoints="true"/>
			<EventFrame parentKey="ScrollBar" inherits="MinimalScrollBar">
				<Anchors>
					<Anchor point="TOP" y="-10"/>
					<Anchor point="BOTTOM" y="10"/>
					<Anchor point="LEFT" relativeKey="$parent.JourneysList" relativePoint="RIGHT" x="-20"/>
				</Anchors>
			</EventFrame>
			<Frame parentKey="BorderFrame" frameLevel="10" inherits="QuestLogBorderFrameTemplate">
				<Size x="75" y="25"/>
				<Anchors>
					<Anchor point="TOPLEFT" x="0" y="0"/>
					<Anchor point="BOTTOMRIGHT" x="0" y="0"/>
				</Anchors>
			</Frame>
			<!-- Journeys "Progress" Frame - shows the reward track, other features, access to overview page -->
			<Frame parentKey="JourneyProgress" hidden="true" setAllPoints="true" mixin="JourneyProgressFrameMixin">
				<Layers>
					<Layer level="ARTWORK">
						<FontString parentKey="JourneyName" inherits="GameFontHighlightHuge2">
							<Size x="500" y="25"/>
							<Anchors>
								<Anchor point="TOP" y="-15"/>
								<Anchor point="CENTER"/>
							</Anchors>
						</FontString>
						<Texture parentKey="DividerTexture" atlas="ui-journeys-renown-divider" useAtlasSize="true">
							<Anchors>
								<Anchor point="LEFT" x="45"/>
								<Anchor point="RIGHT" x="-45"/>
							</Anchors>
						</Texture>
						<Texture parentKey="DividerGlowTexture" atlas="ui-journeys-renown-divider-glow" useAtlasSize="true" alpha="0.75">
							<Anchors>
								<Anchor point="BOTTOM" relativeKey="$parent.DividerTexture" relativePoint="TOP" y="-8"/>
								<Anchor point="LEFT" x="45"/>
								<Anchor point="RIGHT" x="-45"/>
							</Anchors>
						</Texture>
					</Layer>
				</Layers>
				<Frames>
					<Frame parentKey="LockedStateFrame" mixin="JourneysLockedStateMixin" hidden="true">
						<Size x="170" y="25"/>
						<Anchors>
							<Anchor point="TOP" relativeKey="$parent.JourneyName" relativePoint="BOTTOM" y="-10"/>
							<Anchor point="CENTER"/>
						</Anchors>
						<Layers>
							<Layer level="ARTWORK">
								<Texture parentKey="LockIcon" atlas="ui-journeys-greatvault-lock" useAtlasSize="true">
									<Anchors>
										<Anchor point="LEFT"/>
									</Anchors>
								</Texture>
								<FontString parentKey="JourneyLockedText" inherits="GameFontHighlightLarge" text="JOURNEYS_JOURNEY_LOCKED_LABEL" justifyH="LEFT">
									<Size x="135" y="25"/>
									<Anchors>
										<Anchor point="LEFT" relativeKey="$parent.LockIcon" relativePoint="RIGHT"/>
									</Anchors>
								</FontString>
							</Layer>
						</Layers>
						<Scripts>
							<OnShow method="OnShow"/>
						</Scripts>
					</Frame>
					<Frame parentKey="ProgressDetailsFrame" hidden="true">
						<Size x="270" y="35"/>
						<Anchors>
							<Anchor point="TOP" relativeKey="$parent.JourneyName" relativePoint="BOTTOM" y="-5"/>
							<Anchor point="CENTER"/>
						</Anchors>
						<Layers>
							<Layer level="ARTWORK">
								<Texture parentKey="JourneyLevelBar" atlas="ui-journeys-delve-level-bar">
									<Size x="250" y="25"/>
									<Anchors>
										<Anchor point="LEFT"/>
									</Anchors>
								</Texture>
								<Texture parentKey="JourneyLevelBg" atlas="ui-journeys-delve-level-ring" useAtlasSize="true"> 
									<Anchors>
										<Anchor point="CENTER" relativeKey="$parent.JourneyLevelBar" relativePoint="LEFT" x="0"/>
									</Anchors>
								</Texture>
								<FontString parentKey="JourneyLevel" inherits="GameFontNormalMed2" justifyH="CENTER">
									<Size x="30" y="10"/>
									<Anchors>
										<Anchor point="CENTER" relativeKey="$parent.JourneyLevelBg" x="-2"/>
									</Anchors>
								</FontString>
								<FontString parentKey="JourneyLevelProgress" inherits="GameFontNormalMed2" justifyH="CENTER">
									<Size x="250" y="30"/>
									<Anchors>
										<Anchor point="CENTER" relativeKey="$parent.JourneyLevelBar" relativePoint="CENTER"/>
									</Anchors>
								</FontString>
							</Layer>
						</Layers>
					</Frame>
					<StatusBar parentKey="DelveRewardProgressBar">
						<Size x="648" y="14"/>
						<BarTexture atlas="ui-journeys-progressbar-fill"/>
						<Anchors>
							<Anchor point="CENTER" relativeKey="$parent.ProgressDetailsFrame" relativePoint="BOTTOM" y="-25"/>
						</Anchors>
						<Layers>
							<Layer level="BACKGROUND">
								<Texture parentKey="DelveRewardProgressBarBG" atlas="ui-journeys-progressbar-BG" useAtlasSize="true">
								</Texture>
							</Layer>
							<Layer level="ARTWORK">
								<Texture atlas="ui-journeys-progressbar-frame" useAtlasSize="true"/>
							</Layer>
						</Layers>
					</StatusBar>
					<Button parentKey="OverviewBtn" inherits="JourneyOverviewButtonTemplate" hidden="true"/>
					<!-- Reward track for renown journeys -->
					<Frame parentKey="RenownTrackFrame" inherits="RewardTrackFrameTemplate" hidden="true">
						<KeyValues>
							<KeyValue key="rewardButtonXOffset" value="30" type="number"/>
							<KeyValue key="elementTemplate" value="RenownLevelTemplate" type="string"/>
							<KeyValue key="scrollStartSound" value="SOUNDKIT.UI_MAJOR_FACTION_RENOWN_SLIDE_START" type="global"/>
							<KeyValue key="scrollCenterChangeSound" value="SOUNDKIT.UI_MAJOR_FACTION_RENOWN_SLIDE_START" type="global"/>
						</KeyValues>
						<Anchors>
							<Anchor point="TOP" relativeKey="$parent.ProgressDetailsFrame" relativePoint="BOTTOM" y="-10"/>
						</Anchors>
					</Frame>
					<!-- Reward gtrack for encounter journeys: delves, prey, etc. -->
					<Frame parentKey="EncounterRewardProgressFrame" inherits="RewardProgressFrameTemplate" hidden="true">
						<KeyValues>
							<KeyValue key="elementWidth" value="135" type="number"/>
							<KeyValue key="elementSpacing" value="15" type="number"/>
							<KeyValue key="rewardButtonXOffset" value="30" type="number"/>
							<KeyValue key="elementTemplate" value="RenownLevelCardTemplate" type="string"/>
							<KeyValue key="scrollStartSound" value="SOUNDKIT.UI_MAJOR_FACTION_RENOWN_SLIDE_START" type="global"/>
							<KeyValue key="scrollCenterChangeSound" value="SOUNDKIT.UI_MAJOR_FACTION_RENOWN_SLIDE_START" type="global"/>
						</KeyValues>
						<Anchors>
							<Anchor point="TOP" relativeKey="$parent.ProgressDetailsFrame" relativePoint="BOTTOM" y="-5"/>
						</Anchors>
					</Frame>
					<Frame parentKey="DelvesCompanionConfigurationFrame" hidden="true">
						<Size x="310" y="85"/>
						<Anchors>
							<Anchor point="TOP" relativeKey="$parent.DividerTexture" relativePoint="BOTTOM" y="-25"/>
							<Anchor point="CENTER"/>
						</Anchors>
						<Frames>
							<Button parentKey="CompanionConfigBtn" inherits="AlphaHighlightButtonTemplate" text="JOURNEYS_COMPANION_CONFIG_LABEL" mixin="JourneyCompanionConfigBtnMixin" motionScriptsWhileDisabled="true" hidden="true">
								<Size x="313" y="103"/>
								<NormalTexture parentKey="NormalTexture" atlas="ui-journeys-delve-companion-button" useAtlasSize="true"/>
								<PushedTexture parentKey="PushedTexture" atlas="ui-journeys-delve-companion-button-pressed" useAtlasSize="true"/>
