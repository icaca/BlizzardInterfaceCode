<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\Blizzard_SharedXML\UI.xsd">
	<Frame name="EncounterTimeline" mixin="EncounterTimelineMixin" inherits="EditModeEncounterEventsSystemTemplate" parent="UIParent" hidden="true">
		<Size x="1" y="1"/>
		<KeyValues>
			<KeyValue key="systemIndex" value="Enum.EditModeEncounterEventsSystemIndices.Timeline" type="global"/>
			<KeyValue key="systemNameString" value="HUD_EDIT_MODE_SYSTEM_ENCOUNTER_TIMELINE" type="global"/>
		</KeyValues>
		<Frames>
			<Frame parentKey="TimelineView" inherits="EncounterTimelineViewTemplate">
				<Anchors>
					<Anchor point="TOPLEFT"/>
				</Anchors>
			</Frame>
		</Frames>
		<Animations>
			<AnimationGroup parentKey="ShowAnimation" setToFinalAlpha="true">
				<Alpha parentKey="Alpha" duration="0.2" fromAlpha="0" toAlpha="1" smoothing="IN"/>
			</AnimationGroup>
			<AnimationGroup parentKey="HideAnimation" setToFinalAlpha="true" inherits="TargetsHiddenOnFinishedAnimGroupTemplate">
				<Alpha parentKey="Alpha" duration="0.2" startDelay="0.3" fromAlpha="1" toAlpha="0" smoothing="OUT"/>
			</AnimationGroup>
		</Animations>
		<Scripts>
			<OnLoad method="OnLoad"/>
			<OnEvent method="OnEvent"/>
		</Scripts>
	</Frame>
</Ui>
