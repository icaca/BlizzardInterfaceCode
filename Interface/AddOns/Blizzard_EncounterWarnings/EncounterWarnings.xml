<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\Blizzard_SharedXML\UI.xsd">

	<Frame name="EncounterWarningsSystemFrameTemplate" mixin="EncounterWarningsSystemFrameMixin" inherits="EditModeEncounterEventsSystemTemplate" virtual="true">
		<Size x="1" y="1"/>
		<Attributes>
			<Attribute name="explicitlyShown" value="false" type="boolean"/>
		</Attributes>
		<Frames>
			<Frame parentKey="View" inherits="EncounterWarningsViewTemplate">
				<Anchors>
					<Anchor point="CENTER"/>
				</Anchors>
			</Frame>
		</Frames>
		<Scripts>
			<OnLoad method="OnLoad"/>
			<OnShow method="OnShow"/>
			<OnHide method="OnHide"/>
			<OnEvent method="OnEvent"/>
		</Scripts>
	</Frame>

	<Frame name="CriticalEncounterWarnings" inherits="EncounterWarningsSystemFrameTemplate" parent="UIParent">
		<Size x="600" y="48"/>
		<KeyValues>
			<KeyValue key="systemIndex" value="Enum.EditModeEncounterEventsSystemIndices.CriticalWarnings" type="global"/>
			<KeyValue key="systemNameString" value="HUD_EDIT_MODE_SYSTEM_ENCOUNTER_CRITICAL_WARNINGS" type="global"/>
		</KeyValues>
	</Frame>

	<Frame name="MediumEncounterWarnings" inherits="EncounterWarningsSystemFrameTemplate" parent="UIParent">
		<Size x="550" y="36"/>
		<KeyValues>
			<KeyValue key="systemIndex" value="Enum.EditModeEncounterEventsSystemIndices.MediumWarnings" type="global"/>
			<KeyValue key="systemNameString" value="HUD_EDIT_MODE_SYSTEM_ENCOUNTER_MEDIUM_WARNINGS" type="global"/>
		</KeyValues>
	</Frame>

	<Frame name="MinorEncounterWarnings" inherits="EncounterWarningsSystemFrameTemplate" parent="UIParent">
		<Size x="500" y="36"/>
		<KeyValues>
			<KeyValue key="systemIndex" value="Enum.EditModeEncounterEventsSystemIndices.NormalWarnings" type="global"/>
			<KeyValue key="systemNameString" value="HUD_EDIT_MODE_SYSTEM_ENCOUNTER_NORMAL_WARNINGS" type="global"/>
		</KeyValues>
	</Frame>
</Ui>
