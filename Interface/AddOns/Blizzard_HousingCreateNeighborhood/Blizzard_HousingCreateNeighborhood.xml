	<Frame name="HousingCreateCharterNeighborhoodConfirmationFrame" inherits="HousingCreateNeighborhoodConfirmationTemplate" mixin="HousingCreateCharterNeighborhoodConfirmationMixin" parent="UIParent" toplevel="true" enableMouse="true" frameStrata="DIALOG" hidden="true">
		<Scripts>
			<OnLoad inherit="append" method="OnLoad"/>
			<OnShow method="OnShow"/>
			<OnHide method="OnHide"/>
			<OnEvent method="OnEvent"/>
		</Scripts>
		<Size x="600" y="350"/>
	</Frame>

	<Frame name="HousingCreateGuildNeighborhoodFrame" inherits="HousingCreateNeighborhoodTemplate" mixin="HousingCreateGuildNeighborhoodMixin" parent="UIParent" movable="true" enableMouse="true" hidden="true">
		<Scripts>
			<OnLoad inherit="append" method="OnLoad"/>
			<OnHide method="OnHide"/>
			<OnEvent inherit="append" method="OnEvent"/>
			<OnShow inherit="append" method="OnShow"/>
		</Scripts>
		<Layers>
			<Layer level="ARTWORK">
				<FontString parentKey="GuildLabel" inherits="Game15Font_Shadow" text="HOUSING_CREATENEIGHBORHOOD_GUILD">
					<Anchors>
						<Anchor point="TOPLEFT" relativeKey="$parent.LocationLabel" relativePoint="BOTTOMLEFT" y="-8"/>
					</Anchors>
					<Color r="1.0" g="0.82" b="0"/>
				</FontString>
				<FontString parentKey="GuildText" inherits="Game15Font_Shadow" text="guild name here">
					<Anchors>
						<Anchor point="LEFT" relativeKey="$parent.GuildLabel" relativePoint="RIGHT" x="5"/>
					</Anchors>
				</FontString>
				<FontString parentKey="NeighborhoodRequirementsError" inherits="Game15Font_Shadow" text="HOUSING_CREATENEIGHBORHOOD_ERROR_BADNAME" hidden="false">
					<Anchors>
						<Anchor point="BOTTOM" y="57"/>
					</Anchors>
					<Color color="PURE_RED_COLOR"/>
				</FontString>
			</Layer>
		</Layers>
		<Frames>
			<Frame parentKey="ConfirmationFrame" inherits="HousingCreateNeighborhoodConfirmationTemplate" mixin="HousingCreateGuildNeighborhoodConfirmationMixin" setAllPoints="true" toplevel="true" enableKeyboard="true" enableMouse="true" frameStrata="DIALOG" hidden="true">
				<Scripts>
					<OnLoad inherit="append" method="OnLoad"/>
					<OnShow method="OnShow"/>
				</Scripts>
				<Layers>
					<Layer level="ARTWORK">
						<FontString parentKey="GuildLabel" inherits="Game15Font_Shadow" text="HOUSING_CREATENEIGHBORHOOD_GUILD">
							<Anchors>
								<Anchor point="TOPLEFT" relativeKey="$parent.LocationLabel" relativePoint="BOTTOMLEFT" y="-8"/>
							</Anchors>
							<Color r="1.0" g="0.82" b="0"/>
						</FontString>
						<FontString parentKey="GuildText" inherits="Game15Font_Shadow" justifyH="LEFT" text="guild name here">
							<Anchors>
								<Anchor point="LEFT" relativeKey="$parent.GuildLabel" relativePoint="RIGHT" x="2"/>
							</Anchors>
						</FontString>
					</Layer>
				</Layers>
			</Frame>
		</Frames>
	</Frame>

	<Frame name="HousingCreateNeighborhoodCharterFrame" inherits="HousingCreateNeighborhoodTemplate" mixin="HousingCreateNeighborhoodCharterMixin" parent="UIParent" movable="true" enableMouse="true" hidden="true">
		<Scripts>
			<OnLoad inherit="append" method="OnLoad"/>
			<OnHide method="OnHide"/>
			<OnEvent inherit="append" method="OnEvent"/>
			<OnShow inherit="append" method="OnShow"/>
		</Scripts>
		<Layers>
			<Layer level="ARTWORK">
				<FontString parentKey="CharterSettingsWarning" inherits="Game15Font_Shadow" text="HOUSING_CREATENEIGHBORHOOD_SETTINGS_WARNING" hidden="true">
					<Anchors>
						<Anchor point="BOTTOM" y="65"/>
					</Anchors>
					<Color color="PURE_RED_COLOR"/>
				</FontString>
			</Layer>
		</Layers>
	</Frame>
</Ui>