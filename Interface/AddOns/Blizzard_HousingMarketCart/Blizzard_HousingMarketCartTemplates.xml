<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\Blizzard_SharedXML\UI.xsd">
	
	<Button name="HousingMarketShoppingCartServiceButtonTemplate" inherits="ShoppingCartServiceButtonTemplate" virtual="true">
		<KeyValues>
			<KeyValue key="eventNamespace" value="HOUSING_MARKET_EVENT_NAMESPACE" type="global"/>
		</KeyValues>
	</Button>
	
	<Frame name="HousingMarketCartFrame" inherits="ShoppingCartVisualsFrameTemplate" mixin="HousingMarketCartFrameMixin" parent="UIParent">
		<Size x="400" y="500"/>
		<KeyValues>
			<KeyValue key="eventNamespace" value="HOUSING_MARKET_EVENT_NAMESPACE" type="global"/>
			<KeyValue key="maxItemsToShow" value="5" type="number"/>
			<KeyValue key="spacingSize" value="6" type="number"/>
			<KeyValue key="sizingItemTemplate" value="HousingMarketCartItemTemplate" type="string"/>
		</KeyValues>
		<Anchors>
			<Anchor point="BOTTOM" relativePoint="BOTTOM" x="350" y="250"/>
		</Anchors>

		<Scripts>
			<OnLoad method="OnLoad"/>
		</Scripts>
	</Frame>
	
</Ui>
