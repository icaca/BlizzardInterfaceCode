<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\Blizzard_SharedXML\UI.xsd">
	<Button name="BaseHousingCatalogEntryTemplate" mixin="HousingCatalogEntryMixin" registerForDrag="LeftButton" registerForClicks="LeftButtonUp,RightButtonUp" motionScriptsWhileDisabled="true" virtual="true">
		<KeyValues>
			<KeyValue key="backgroundDefault" value="house-chest-list-Item-default" type="string" />
			<KeyValue key="backgroundActive" value="house-chest-list-Item-active" type="string" />
			<KeyValue key="backgroundPressed" value="house-chest-list-Item-pressed" type="string" />
		</KeyValues>
		<Size x="97" y="97"/>
		<Layers>
			<Layer level="BACKGROUND" textureSubLevel="0">
				<Texture parentKey="Background" atlas="house-chest-list-item-default" setAllPoints="true">
				</Texture>
			</Layer>
			<Layer level="BACKGROUND" textureSubLevel="0">
				<Texture parentKey="HoverBackground" atlas="house-chest-list-item-default" setAllPoints="true" alpha="0.75" alphaMode="ADD" hidden="true">
				</Texture>
			</Layer>
			<Layer level="ARTWORK">
				<Texture parentKey="Icon">
					<Anchors>
						<Anchor point="TOPLEFT" x="5" y="-5"/>
						<Anchor point="BOTTOMRIGHT" x="-5" y="5"/>
					</Anchors>
				</Texture>
			</Layer>
			<Layer level="OVERLAY" textureSubLevel="3">
				<FontString parentKey="InfoText" inherits="GameFontHighlightLarge">
					<Size y="20"/>
					<Anchors>
						<Anchor point="BOTTOMRIGHT" x="-9" y="7"/>
					</Anchors>
				</FontString>
			</Layer>
		</Layers>
		<Frames>
			<ModelScene parentKey="ModelScene" inherits="NonInteractableModelSceneMixinTemplate">
				<Size x="80" y="80"/>
				<Anchors>
					<Anchor point="CENTER"/>
				</Anchors>
			</ModelScene>
		</Frames>
		<Scripts>
			<OnLoad method="OnLoad"/>
			<OnEnter method="OnEnter"/>
			<OnLeave method="OnLeave"/>
			<OnClick method="OnClick"/>
			<OnDragStart method="OnDragStart"/>
			<OnMouseDown method="OnMouseDown"/>
			<OnMouseUp method="OnMouseUp"/>
		</Scripts>
	</Button>

	<Button name="HousingCatalogDecorEntryTemplate" mixin="HousingCatalogDecorEntryMixin" inherits="BaseHousingCatalogEntryTemplate" virtual="true"/>
	<Button name="HousingCatalogRoomEntryTemplate" mixin="HousingCatalogRoomEntryMixin" inherits="BaseHousingCatalogEntryTemplate" virtual="true">
		<Scripts>
			<OnEvent method="OnEvent"/>
		</Scripts>
	</Button>
</Ui>